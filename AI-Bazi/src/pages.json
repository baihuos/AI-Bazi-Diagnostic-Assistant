{
	"easycom": {
		"autoscan": true,
		"custom": {
			"^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue",
			"^Xtx(.*)": "@/components/Xtx$1.vue"
		}
	},
	"pages": [
		{
			"path": "pages/login/register"
		},
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "算算",
				"navigationBarBackgroundColor": "#000000",
				"navigationBarTextStyle": "white"
			}
		},
		{
			"path": "pages/login/login",
			"style": {
				"navigationBarTitleText": "登录"
			}
		},
		{
			"path": "pages/login/start",
			"style": {
				"navigationBarTitleText": "开始"
			}
		},
		{
			"path": "pages/login/guide",
			"style": {
				"navigationBarTitleText": "引导"
			}
		},
		{
			"path": "pages/face/face",
			"style": {
				"navigationBarTitleText": "人脸识别"
			}
		},
		{
			"path": "pages/ActivityLogs/ActivityLogs",
			"style": {
				"navigationBarTitleText": "历史"
			}
		},
		{
			"path": "pages/Invitation/Invitation",
			"style": {
				"navigationBarTitleText": "邀请您的朋友"
			}
		},
		{
			"path": "pages/record/record",
			"style": {
				"navigationBarTitleText": "记录"
			}
		},
		{
			"path":"pages/signin/signin",
			"style":{
				"navigationBarTitleText":"每日签到"
			}
		},
		{
			"path": "pages/my/my",
			"style": {
				"navigationBarTitleText": "我的"
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#000000",
		"backgroundColor": "#000000"
	},
	"tabBar": {
		"color": "#333",
		"selectedColor": "#5e5c5c",
		"backgroundColor": "#000000",
		"borderStyle": "white",
		"list": [
			{
				"text": "算算",
				"pagePath": "pages/index/index",
				"iconPath": "static/tabs/home_default.png",
				"selectedIconPath": "static/tabs/home_selected.png"
			},
			{
				"text": "我的",
				"pagePath": "pages/my/my",
				"iconPath": "static/tabs/user_default.png",
				"selectedIconPath": "static/tabs/user_selected.png"
			}
		]
	},
	"subPackages": [
		{
			"root": "pagesMember",
			"pages": [
				{
					"path": "pages/member/member",
					"style": {
						"navigationBarTitleText": "会员"
					}
				}
			]
		}
	],
	"preloadRule": {
		"pages/my/my": {
			"network": "all",
			"packages": [
				"pagesMember"
			]
		}
	}
}